 27  PROCEDURE showItemOrders(orderid IN OUT number) IS
 28  BEGIN
 29  	b:=0;
 30  
 31  	select CustId into custid
 32  	from CustOrder
 33  	where OrderId=orderid;
 34  
 35   select name,email,CustType,address into name1,email,custtype,address
 36  	from Customers
 37  	where CustId=custid;
 38  	dbms_output.put_line('Customer ID: '||custid||'  '||'Name: '||name1||'	'||'EmailID: '||email||'  '||'Customer_type: '||custtype||'  '||'Address: '||address);
