 48  	   EXIT WHEN details%notfound;
 49  	   if orderid=orderid1 then
 50  
 51  	   select price into price1
 52  	   from StoreItems
 53  	   where ItemId=itemid;
 54  
 55  	   b:=b+price1*quantity;
